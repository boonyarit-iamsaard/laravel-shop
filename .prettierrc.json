{
    "plugins": ["prettier-plugin-blade", "prettier-plugin-tailwindcss"],
    "tabWidth": 4,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": true,
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.blade.php",
            "options": {
                "parser": "blade"
            }
        }
    ]
}
